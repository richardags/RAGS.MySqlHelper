<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Version>1.0.2</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>RAGS.MySqlHelper</PackageId>
    <Title>MySqlHelper</Title>
    <Authors>richardags</Authors>
    <Description>This is wrapper on MySqlConnection that brings interface implementation and encapsulates the original MySqlConnection.</Description>
    <Copyright>MIT</Copyright>
    <RepositoryUrl>https://github.com/richardags/RAGS.MySqlHelper</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MySql.Data" Version="8.0.30" />
  </ItemGroup>

</Project>
